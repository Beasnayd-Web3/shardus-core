{
  "name": "@shardus/core",
  "version": "2.9.19",
  "description": "The Shardus Global Server",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/*"
  ],
  "config": {
    "dist": "./dist"
  },
  "scripts": {
    "lint": "eslint './src/**/*.ts'",
    "lint-windows": "eslint ./src/**/*.ts",
    "ensure:node-version": "node -e \"'v' + require('./package.json').engines.node == process.version || process.exit(1)\"",
    "clean": "shx rm -rf dist build",
    "create:folders": "shx mkdir -p dist",
    "compile:ts": "tsc",
    "compile:pack": "webpack",
    "compile:js": "bytenode -c build/src/index.js",
    "release:move:typedocs": "shx mv build/src/index.d.ts dist/",
    "release:move:extras": "shx mv build/src/crypto/computePowGenerator.js dist/ && shx cp templates/release.index.js dist/index.js",
    "dev:move": "shx mv build/src dist",
    "prep": "npm run create:folders",
    "np": "np --any-branch --no-cleanup --no-tests --no-yarn --message=\"Bump version to %s\"",
    "compile": "npm-run-all compile:*",
    "release:move": "npm-run-all release:move:*",
    "build:dev": "npm-run-all clean compile:ts dev:move",
    "build:release": "npm-run-all clean prep compile release:move",
    "release": "npm-run-all ensure:* build:release np",
    "check": "gts check",
    "fix": "gts fix",
    "test": "jest",
    "test-watch": "jest --watch",
    "depcheck": "depcheck --skip-missing --ignores webpack,webpack-cli,prettier,typescript-eslint,typescript-estree"
  },
  "repository": {
    "type": "git",
    "url": "git@gitlab.com:shardus/global/shardus-global-server.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@hapi/sntp": "^4.0.0",
    "@shardus/crypto-utils": "^4.0.4",
    "@shardus/net": "1.1.0",
    "@shardus/types": "^1.0.13",
    "body-parser": "^1.20.2",
    "bytenode": "^1.4.1",
    "cors": "^2.8.5",
    "deepmerge": "^4.3.1",
    "express": "^4.18.2",
    "fast-stable-stringify": "^1.0.0",
    "got": "^13.0.0",
    "log4js": "^6.9.1",
    "log4js-extend": "^0.2.1",
    "nat-api": "^0.3.1",
    "sequelize": "^6.32.0",
    "socket.io": "^4.6.2",
    "sqlite3": "^5.1.6",
    "streamroller": "^3.1.5",
    "tar-fs": "^2.1.1",
    "trie-prefix-tree": "^1.5.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/got": "^9.6.12",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/socket.io": "^3.0.1",
    "@types/tar-fs": "^2.0.1",
    "@types/trie-prefix-tree": "^1.5.0",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "bytenode-webpack-plugin": "^1.0.5",
    "depcheck": "^1.4.3",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-no-unsanitized": "^4.0.2",
    "eslint-plugin-security": "^1.7.1",
    "eslint-plugin-xss": "^0.1.12",
    "jest": "^29.5.0",
    "np": "^8.0.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "replace-in-file": "^7.0.1",
    "shx": "^0.3.4",
    "ts-jest": "^29.1.0",
    "typescript": "^5.1.3",
    "typescript-eslint": "^0.0.1-alpha.0",
    "typescript-estree": "^18.1.0",
    "webpack": "^5.87.0",
    "webpack-cli": "^5.1.4",
    "webpack-node-externals": "^3.0.0"
  }
}
